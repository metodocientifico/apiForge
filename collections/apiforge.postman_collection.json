{
    "info": {
      "name": "APIForge",
      "description": "Colecci√≥n de pruebas para APIForge",
      "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
      {
        "name": "Obtener lista de recursos con token JWT",
        "request": {
          "method": "GET",
          "header": [
            {
              "key": "Authorization",
              "value": "Bearer VALID_JWT_TOKEN",
              "type": "text"
            }
          ],
          "url": {
            "raw": "http://localhost:4010/resources",
            "protocol": "http",
            "host": ["localhost"],
            "port": "4010",
            "path": ["resources"]
          }
        }
      },
      {
        "name": "Obtener lista de recursos sin token",
        "request": {
          "method": "GET",
          "url": {
            "raw": "http://localhost:4010/resources",
            "protocol": "http",
            "host": ["localhost"],
            "port": "4010",
            "path": ["resources"]
          }
        }
      },
      {
        "name": "Obtener lista de recursos con API Key",
        "request": {
          "method": "GET",
          "header": [
            {
              "key": "X-API-Key",
              "value": "VALID_API_KEY",
              "type": "text"
            }
          ],
          "url": {
            "raw": "http://localhost:4010/resources",
            "protocol": "http",
            "host": ["localhost"],
            "port": "4010",
            "path": ["resources"]
          }
        }
      }
    ]
  }
  